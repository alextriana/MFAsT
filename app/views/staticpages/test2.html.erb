<script>
    $('#submit-subscribe').click(function() {
    $('#subscribe-confirmed').show();
    });

    $('#advisors').click(function() {
    $('#founders-block').slideUp();
    $('#advisor-block').slideDown();
    });

    $('#founders').click(function() {
    $('#advisor-block').slideUp();
    $('#founders-block').slideDown();
    });

    $('#submit-feedback').click(function() {
    $('#feedback-confirmed').show();
    });
    function unloadPopupBox() { // TO Unload the Popupbox
      $('#popup_box').fadeOut("slow");
      $("#content").css({ // this is just for style   
        "opacity": "1"  
      }); 
    }

    
    function loadPopupBox() { // To Load the Popupbox
  
      $('#popup_box').fadeIn("slow");
      $("#content").css({ // this is just for style
        "opacity": "0.3"  });
      $('#form-popup').show();
      
     
  }

    $("#beta").click(function () {

      loadPopupBox();
    });

    $('#beta-no').click(function(){
      unloadPopupBox();
    });
    $('#close-beta').click(function(){
  
    $('#popup-submission').hide();
    unloadPopupBox();

  
    });

    $('#submit-beta').click(function() {
      console.log('hit');
      $('#form-popup').hide();
      $('#popup-submission').show();
    });



</script>
    
